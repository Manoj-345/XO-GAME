{
  "family": "xo-game-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::228907232010:role/ecsTaskExecutionRoles",
  "containerDefinitions": [
    {
      "name": "xo-game-container",
      "image": "228907232010.dkr.ecr.ap-south-1.amazonaws.com/xo-game:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
